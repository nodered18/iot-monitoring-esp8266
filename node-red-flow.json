        "id": "467e77cc759e8512",
        "type": "function",
        "z": "dd908c5c21835aed",
        "name": "Préparer données",
        "func": "// Parser les données JSON envoyées par MQTT\nvar data = JSON.parse(msg.payload);\n\n// Préparer le bon format pour Google Sheets (avec les noms correspondants au script)\nmsg.headers = {\n    \"Content-Type\": \"application/json\"\n};\n\nmsg.payload = {\n    voltage: data.voltage,\n    current: data.current,\n    power: data.power,\n    energy: data.energy,\n    temperature: data.temperature,\n    frequency: data.frequency,\n    pf: data.pf, // ✅ nom resté 'pf' pour correspondre au script Google\n    irradiance: data.irradiance\n};\n\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
